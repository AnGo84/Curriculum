<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="210.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ua.curriculum.controller.ChangePasswordViewController">
    <children>
        <Label layoutX="24.0" layoutY="14.0" text="Поточний пароль:" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="10.0" />
        <Label layoutX="24.0" layoutY="62.0" text="Новий пароль" AnchorPane.leftAnchor="15.0" />
        <Label layoutX="24.0" layoutY="110.0" text="Повторіть пароль" AnchorPane.leftAnchor="15.0" />

        <PasswordField fx:id="passwordFPassword" layoutX="24.0" layoutY="30.0" prefWidth="200.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
        <PasswordField fx:id="passwordFNewPassword" layoutX="24.0" layoutY="80.0" prefWidth="200.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
        <PasswordField fx:id="passwordFRepeatPassword" layoutX="24.0" layoutY="130.0" prefWidth="200.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
        <Button fx:id="buttonOk" defaultButton="true" layoutX="168.0" layoutY="124.0" mnemonicParsing="false" onAction="#onButtonOk" prefWidth="110.0" text="Підтвердити" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />
        <Button fx:id="buttonCancel" layoutX="168.0" layoutY="124.0" mnemonicParsing="false" onAction="#onButtonCancel" prefWidth="110.0" text="Відмінити" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" />
    </children>
</AnchorPane>
